{
  "tsserver.enable": true,
  "rust-analyzer.server.path": "~/.cargo/bin/rust-analyzer",
  "rust-client.logToFile": "~/coc-rls.log",
  "rust-analyzer.debug.runtime": "vimspector",
  "rust-analyzer.debug.vimspector.configuration.name": "lldb",
  "toml.schema.associations": {
      ".*/Cargo\\.toml": "https://taplo.tamasfe.dev/schemas/cargo.toml.json"
  },
  "languageserver": {
     "clangd": {
       "command": "clangd",
       "rootPatterns": ["compile_commands.json"],
       "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"]
     }
   }
}
